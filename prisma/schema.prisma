generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DB_URL")
}

model Reserva {
  id               Int     @id @default(autoincrement())
  numeroMesa       Int
  data             String
  hora             String
  nomeResponsavel  String  @unique
  statusUtilizadas Boolean
  garcom           String
}

model Mesa {
  numero Int     @id
  status Boolean
}
